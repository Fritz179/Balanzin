{
  "compilerOptions": {
    "target": "es2021",
    "outDir": "./out",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "paths": {},
    "alwaysStrict": true,
    "allowUnreachableCode": false,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitReturns": true,
    "noUncheckedIndexedAccess": true,
    "noUnusedLocals": true
  },
}